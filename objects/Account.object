    <validationRules>
        <fullName>Account_Number_8_Characters</fullName>
        <active>false</active>
        <errorConditionFormula>LEN(AccountNumber ) != 8</errorConditionFormula>
        <errorMessage>Account number must be 8 characters long</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Number_is_Numeric</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(AccountNumber)),
NOT(ISNUMBER(AccountNumber))
				)</errorConditionFormula>
        <errorMessage>Account Number should be Numeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_country_code</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
LEN (BillingCountry) = 1,

NOT(
 CONTAINS(
					
					&quot;IN&quot; &amp;
					&quot;IT&quot;,
					 BillingCountry )))</errorConditionFormula>
        <errorMessage>A valid two-letter country code is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_must_be_in_current_year_date</fullName>
        <active>false</active>
        <errorConditionFormula>YEAR(My_Date__c ) &lt;&gt; YEAR (TODAY())</errorConditionFormula>
        <errorMessage>DATE SHOULD BE IN A CURRENT YEAR</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Minimum_Spending_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
 (ISPICKVAL( Support_Tier__c , &quot;Silver&quot;) 
      &amp;&amp; 
   ( Total_Spending__c &lt;  
   $CustomMetadata.Support_Tier__mdt.Silver.Minimum_Spending__c 
   )
 ) 
      || 
 (ISPICKVAL( Support_Tier__c , &quot;Gold&quot;) 
      &amp;&amp; 
   ( Total_Spending__c &lt;  
   $CustomMetadata.Support_Tier__mdt.Gold.Minimum_Spending__c 
   )
 ), TRUE, FALSE
)</errorConditionFormula>
        <errorMessage>Dollar value too low for this support tie</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Revenue_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
(ISPICKVAL( Support_Tier__c , &quot;Gold&quot;)
&amp;&amp;
( AnnualRevenue &gt;=
20*$CustomMetadata.Support_Tier__mdt.Gold.Minimum_Spending__c
)
), TRUE, FALSE
)</errorConditionFormula>
        <errorMessage>ERROR</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>website</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

RIGHT(website__c,4) &lt;&gt; &quot;.COM&quot;,
RIGHT(website__c,4) &lt;&gt; &quot;.org&quot;				


)</errorConditionFormula>
        <errorMessage>website error</errorMessage>
    </validationRules>
